    %Training Data
    TRIALS = 10;
    if trained2 == 1 %Now we will use LR instead of Ryan's guess
        P_eyeopen = 1/(1+exp(-(dot(B,[1 covariance(2,:)]))))
        if (P_eyeopen > 0.9)
            eyesopen_LR(count) = 1
        else
            eyesopen_LR(count) = 0
        end
    end        
    if trained2 == 0 
        if count == TRIALS %Fit to Ryan's prediction
            powertrain(count,:)=alphapower(count,7);
            B = mnrfit(powertrain,blink+1);
            if (isempty(B)) %Check if the LR worked
                fprintf('Not enough eye open/closed data, retrying regression. \n');
                %count = 0;
                break;
            else
                fprintf('Regression successful for eyes open/closed, now using LR. \n');
            end
            trained2 = 1;
        else
            powertrain(count,:)=alphapower(count,7);
        end
    end    
    %Outputs 
   
